    "@mui/material": "^5.14.5",
    "@rehooks/online-status": "^1.1.2",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/react-query": "^1.2.9",
    "axios": "^0.26.0",
    "date-fns": "^2.28.0",
    "google-map-react": "^2.1.10",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
