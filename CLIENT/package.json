    "@babel/core": "^7.17.5",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-angular": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@honkhonk/vite-plugin-svgr": "^1.1.0",
